CREATE VIEW SRU2_DORM_INFO
AS 
SELECT E1.*,E3.INSTITUTENAME,E4.DORMNAME,E5.ZUTUANID,E6.CAMPUSNAME
FROM STUDENT E1,MAJOR E2,INSTITUTE E3,DORM E4,DORM_BUILDING E5,CAMPUS E6
WHERE E1.MAJORID=E2.MAJORID AND E2.MAJORLOCALINSTITUTEID=E3.INSTITUTEID AND E1.DORMID=E4.DORMID AND E4.DORM_BUILDINGID=E5.DORM_BUILDINGID AND E5.CAMPUSID=E6.CAMPUSID;

ALTER VIEW SRU2_DORM_INFO
AS
SELECT E1.*,E3, E1.BIRTHDAY, E3.COLLEGENAME, E4.DORMNAME, E5.ZUTUANID, E6.CAMPUSNAME
FROM STUDENT E1, PROFESSIONAL E2, COLLEGE E3, DORM E4, DORMBUILDING E5, CAMPUS E6
WHERE E1.PROFESSIONALID = E2.PROFESSIONALID AND E2.PROFESSIONALLOCALCOLLEGEID = E3.COLLEGEID AND E1.DORMID = E4.DORMID AND E4.DORMBUILDINGID = E5.DORMBUILDINGID AND E5.CAMPUSID = E6.CAMPUSID;



